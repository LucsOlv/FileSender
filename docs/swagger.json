{
    "swagger": "2.0",
    "info": {
        "description": "API para upload de arquivos",
        "title": "API de Upload de Arquivos",
        "contact": {},
        "version": "1.0"
    },
    "host": "localhost:8080",
    "basePath": "/api",
    "paths": {
        "/form": {
            "post": {
                "description": "Faz upload de múltiplos arquivos",
                "consumes": [
                    "multipart/form-data"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "uploads"
                ],
                "summary": "Upload de arquivos",
                "parameters": [
                    {
                        "type": "file",
                        "description": "Arquivos para upload (múltiplos)",
                        "name": "files",
                        "in": "formData",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    },
                    "400": {
                        "description": "Nenhum arquivo foi enviado",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        }
    }
}